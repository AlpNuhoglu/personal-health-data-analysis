    "# 2.1 HISTOGRAMS\n",
    "hist_weight = alt.Chart(df).mark_bar().encode(\n",
    "    alt.X('Weight(kg):Q', bin=alt.Bin(step=1), title='Weight(kg)'),\n",
    "    alt.Y('count()', title='Frequency'),\n",
    "    tooltip=['count()']\n",
    ").properties(\n",
    "    width=250,\n",
    "    height=200,\n",
    "    title='Weight Distribution (bin=1 kg)'\n",
    ")\n",
    "\n",
    "hist_steps = alt.Chart(df).mark_bar().encode(\n",
    "    alt.X('Step_count:Q', bin=alt.Bin(step=1000), title='Step_count'),\n",
    "    alt.Y('count()', title='Frequency'),\n",
    "    tooltip=['count()']\n",
    ").properties(\n",
    "    width=250,\n",
    "    height=200,\n",
    "    title='Step Count Distribution (bin=1000)'\n",
    ")\n",
    "\n",
    "hist_calories = alt.Chart(df).mark_bar().encode(\n",
    "    alt.X('Calorie(kcal):Q', bin=alt.Bin(step=200), title='Calorie(kcal)'),\n",
    "    alt.Y('count()', title='Frequency'),\n",
    "    tooltip=['count()']\n",
    ").properties(\n",
    "    width=250,\n",
    "    height=200,\n",
    "    title='Calorie Distribution (bin=200)'\n",
    ")\n",
    "\n",
    "histograms = hist_weight | hist_steps | hist_calories\n",
    "histograms"
   ]
  },
