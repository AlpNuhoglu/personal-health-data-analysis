    "# Convert 'Gym' to 1/0\n",
    "df['Gym_Bool'] = df['Gym'].apply(lambda x: 1 if x=='Yes' else 0)\n",
    "\n",
    "# Our input features\n",
    "X = df[['Step_count','Calorie(kcal)','Gym_Bool']]\n",
    "# Our target\n",
    "y = df['Weight(kg)']\n",
    "\n",
    "# Train/test split\n",
    "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.3, random_state=42)\n",
    "\n",
    "# Instantiate and fit linear regression\n",
    "linreg_model = LinearRegression()\n",
    "linreg_model.fit(X_train, y_train)\n",
    "\n",
    "# Predict\n",
    "y_pred_lin = linreg_model.predict(X_test)\n",
    "\n",
    "# Evaluate\n",
    "mse_lin = mean_squared_error(y_test, y_pred_lin)\n",
    "r2_lin = r2_score(y_test, y_pred_lin)\n",
    "\n",
    "print(\"Multiple Linear Regression Results:\")\n",
    "print(\"  MSE:\", round(mse_lin,3))\n",
    "print(\"  RMSE:\", round(np.sqrt(mse_lin),3))\n",
    "print(\"  R^2:\", round(r2_lin,3))\n",
    "\n",
    "# Coefficients\n",
    "coeffs = pd.DataFrame({\n",
    "    'Feature': X.columns,\n",
    "    'Coefficient': linreg_model.coef_\n",
    "})\n",
    "coeffs['Intercept'] = linreg_model.intercept_\n",
    "coeffs"
