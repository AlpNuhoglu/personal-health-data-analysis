   "source": [
    "# 4.1 Interactive Step Filter\n",
    "import altair as alt\n",
    "\n",
    "min_steps = df['Step_count'].min()\n",
    "max_steps = df['Step_count'].max()\n",
    "\n",
    "# 1) Create a binding_range for the slider\n",
    "step_slider = alt.binding_range(\n",
    "    min=min_steps,\n",
    "    max=max_steps,\n",
    "    step=500,\n",
    "    name='Min Steps'          # Label displayed next to the slider\n",
    ")\n",
    "\n",
    "# 2) Create an Altair parameter using the binding_range\n",
    "step_param = alt.param(\n",
    "    name='step_threshold',    # The internal name for the parameter\n",
    "    bind=step_slider,         # The slider binding\n",
    "    value=min_steps           # Initial slider value\n",
    ")\n",
    "\n",
    "# 3) Build the chart, adding the parameter and transform_filter\n",
    "scatter_filtered = (\n",
    "    alt.Chart(df)\n",
    "    .mark_circle(size=60)\n",
    "    .encode(\n",
    "        x=alt.X('Calorie(kcal):Q'),\n",
    "        y=alt.Y('Weight(kg):Q'),\n",
    "        # Color by Gym = Yes or No:\n",
    "        color=alt.condition(\n",
    "            alt.datum.Gym == 'Yes',\n",
    "            alt.value('#FF6B6B'),\n",
    "            alt.value('#1F77B4')\n",
    "        ),\n",
    "        tooltip=['Days','Weight(kg)','Step_count','Calorie(kcal)']\n",
    "    )\n",
    "    .add_params(step_param)  # Add the parameter to the chart\n",
    "    .transform_filter(\n",
    "        # Filter data points where Step_count >= our step_threshold param\n",
    "        \"datum.Step_count >= step_threshold\"\n",
    "    )\n",
    "    .properties(\n",
    "        width=400,\n",
    "        height=300,\n",
    "        title='Weight vs. Calories (filter by min Step_count)'\n",
    "    )\n",
    ")\n",
    "\n",
    "scatter_filtered\n"
   ]
  },
