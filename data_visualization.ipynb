   "source": [
    "# 3.2 Scatter Plots\n",
    "\n",
    "scatter_weight_cal = alt.Chart(df).mark_circle(size=60).encode(\n",
    "    x=alt.X('Calorie(kcal):Q'),\n",
    "    y=alt.Y('Weight(kg):Q'),\n",
    "    tooltip=['Days','Weight(kg)','Calorie(kcal)','Step_count'],\n",
    "    color=alt.condition(\n",
    "        alt.datum.Gym == 'Yes', \n",
    "        alt.value('red'), \n",
    "        alt.value('blue')\n",
    "    )\n",
    ").properties(\n",
    "    width=300,\n",
    "    height=250,\n",
    "    title='Weight vs. Calorie(kcal)'\n",
    ")\n",
    "\n",
    "scatter_weight_steps = alt.Chart(df).mark_circle(size=60).encode(\n",
    "    x=alt.X('Step_count:Q'),\n",
    "    y=alt.Y('Weight(kg):Q'),\n",
    "    tooltip=['Days','Weight(kg)','Calorie(kcal)','Step_count'],\n",
    "    color=alt.condition(\n",
    "        alt.datum.Gym == 'Yes', alt.value('orange'), alt.value('green')\n",
    "    )\n",
    ").properties(\n",
    "    width=300,\n",
    "    height=250,\n",
    "    title='Weight vs. Step_count'\n",
    ")\n",
    "\n",
    "(scatter_weight_cal | scatter_weight_steps)"
   ]
  },
