    "# 2.2 BOX PLOTS (Univariate) for Outlier Checking\n",
    "box_weight = alt.Chart(df).mark_boxplot(extent='min-max').encode(\n",
    "    y=alt.Y('Weight(kg):Q', title='Weight(kg)')\n",
    ").properties(\n",
    "    width=60,\n",
    "    height=200,\n",
    "    title='Boxplot: Weight(kg)'\n",
    ")\n",
    "\n",
    "box_steps = alt.Chart(df).mark_boxplot(extent='min-max').encode(\n",
    "    y=alt.Y('Step_count:Q', title='Step_count')\n",
    ").properties(\n",
    "    width=60,\n",
    "    height=200,\n",
    "    title='Boxplot: Steps'\n",
    ")\n",
    "\n",
    "box_cal = alt.Chart(df).mark_boxplot(extent='min-max').encode(\n",
    "    y=alt.Y('Calorie(kcal):Q', title='Calorie(kcal)')\n",
    ").properties(\n",
    "    width=60,\n",
    "    height=200,\n",
    "    title='Boxplot: Calories'\n",
    ")\n",
    "\n",
    "boxplots = (box_weight | box_steps | box_cal)\n",
    "boxplots"
   ]
  },
